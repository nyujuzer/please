{
    "builds": [
      {
        "src": "testing/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "testing/wsgi.py"
      }
    ],
    "functions": {
      "testing/**/*.py": {
        "excludeFiles": "media/**"
      }
    }
}